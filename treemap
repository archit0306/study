<script type="text/javascript">

$('#lazy').jstree({
	'core' : {
		'data' : {
			"url" : "https://www.jstree.com/fiddle/?lazy",
			"data" : function (node) {
				return { "id" : node.id };
			}
		}
	}
});





</script>

